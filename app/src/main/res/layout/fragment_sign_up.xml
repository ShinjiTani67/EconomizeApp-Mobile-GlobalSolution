<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignUpFragment">

    <TextView
        android:id="@+id/Logotitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/app_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.113"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.054" />

    <TextView
        android:id="@+id/Cadastrotitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cadastro_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.101"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.127" />

    <TextView
        android:id="@+id/Nometitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/nome_name"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="135dp" />

    <TextView
        android:id="@+id/Senhatitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/confirmarsenha_name"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="521dp" />

    <EditText
        android:id="@+id/Confirmesenhaedittext"
        android:layout_width="295dp"
        android:layout_height="48dp"
        android:ems="10"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="165dp" />

    <EditText
        android:id="@+id/Confirmesenhatitle"
        android:layout_width="295dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="354dp" />

    <EditText
        android:id="@+id/Emailedittext"
        android:layout_width="295dp"
        android:layout_height="48dp"
        android:ems="10"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="452dp" />

    <EditText
        android:id="@+id/senhaedittext"
        android:layout_width="295dp"
        android:layout_height="48dp"
        android:ems="10"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="263dp" />

    <TextView
        android:id="@+id/Confirmeemailtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/confirmaremail_name"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="322dp" />

    <EditText
        android:id="@+id/Confirmemailedittext"
        android:layout_width="295dp"
        android:layout_height="48dp"
        android:ems="10"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="563dp" />

    <TextView
        android:id="@+id/Emailtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/email_name"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="233dp" />

    <TextView
        android:id="@+id/Senhaname_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/senha_name"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="415dp" />

    <Button
        android:id="@+id/confirmarbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/confirmar_name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.928" />
</androidx.constraintlayout.widget.ConstraintLayout>