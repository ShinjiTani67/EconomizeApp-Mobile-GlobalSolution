<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignUpFragment">

    <TextView
        android:id="@+id/Logotitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginTop="44dp"
        android:text="@string/app_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/Cadastrotitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cadastro_name"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/Nometitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cadastro_name"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/Emailtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cadastro_name"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/Confirmeemailtitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cadastro_name"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="103dp" />

    <TextView
        android:id="@+id/Senhatitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/cadastro_name"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="103dp" />

    <EditText
        android:id="@+id/Confirmesenhatitle"
        android:layout_width="295dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="359dp" />

    <EditText
        android:id="@+id/Emailedittext"
        android:layout_width="295dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="359dp" />

    <EditText
        android:id="@+id/Confirmemailedittext"
        android:layout_width="295dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="58dp"
        tools:layout_editor_absoluteY="437dp" />

    <EditText
        android:id="@+id/Senhaedittext"
        android:layout_width="295dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="295dp" />

    <EditText
        android:id="@+id/Confirmesenhaedittext"
        android:layout_width="295dp"
        android:layout_height="48dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="217dp" />


</androidx.constraintlayout.widget.ConstraintLayout>