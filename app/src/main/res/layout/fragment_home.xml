<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeFragment">

    <TextView
        android:id="@+id/Logotitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="44dp"
        android:text="@string/app_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.utils.widget.ImageFilterButton
        android:id="@+id/imageFilterButton"
        android:layout_width="255dp"
        android:layout_height="265dp"
        android:src="@drawable/send_file"
        tools:layout_editor_absoluteX="78dp"
        tools:layout_editor_absoluteY="108dp" />

    <EditText
        android:id="@+id/Senhaedittext"
        android:layout_width="298dp"
        android:layout_height="48dp"
        android:ems="10"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="452dp" />

    <Button
        android:id="@+id/enviarbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/enviar_name"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="623dp" />

    <EditText
        android:id="@+id/Senhaedittext"
        android:layout_width="298dp"
        android:layout_height="48dp"
        android:ems="10"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="552dp" />

    <TextView
        android:id="@+id/Senhatitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/senha_name"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="520dp" />

    <TextView
        android:id="@+id/Senhatitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/senha_name"
        tools:layout_editor_absoluteX="48dp"
        tools:layout_editor_absoluteY="420dp" />

</androidx.constraintlayout.widget.ConstraintLayout>